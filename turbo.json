{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "check-types": { "dependsOn": ["^check-types"] },
    "build": {
      "outputs": ["build-dev-*"],
      "inputs": ["$TURBO_DEFAULT$", ".env.development"],
      "dependsOn": ["^build"]
    },
    "build-prod": {
      "outputs": ["build-prod-*"],
      "inputs": ["$TURBO_DEFAULT$", ".env.production"],
      "dependsOn": ["^build"]
    },
    "dev": {
      "persistent": true,
      "cache": false,
      "dependsOn": ["^check-types"]
    },
    "dev-mock": {
      "persistent": true,
      "cache": false,
      "dependsOn": ["^check-types"]
    },
    "dev-prod": {
      "persistent": true,
      "cache": false,
      "dependsOn": ["^check-types"]
    },
    "dev-typia": {
      "persistent": true,
      "cache": false,
      "dependsOn": ["^check-types"]
    },
    "dev-mock-typia": {
      "persistent": true,
      "cache": false,
      "dependsOn": ["^check-types"]
    },
    "dev-prod-typia": {
      "persistent": true,
      "cache": false,
      "dependsOn": ["^check-types"]
    }
  }
}
